@model IEnumerable<Tracker.Data.tbl_artists>

@{
    ViewBag.Title = "GetArtists";
}

<h2>GetArtists</h2>

<p>
    @Html.ActionLink("Create New", "NewArtist")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Artist_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Artist_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Artist_ID }) |
            @Html.ActionLink("Details", "GetArtistDetails", new { Artist_ID =item.Artist_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Artist_ID })
        </td>
    </tr>
}
</table>
