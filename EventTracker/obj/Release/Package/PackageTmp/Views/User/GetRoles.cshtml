@{
    ViewBag.Title = "GetRoles";
}

<h2>GetRoles</h2>

@foreach (var role in Model)
    {
    <p>
        @role.Name |
        @Html.ActionLink("Edit", "RoleEdit", new { Id = role.Id }) |
        @Html.ActionLink("Delete", "RoleDelete", new { Id = role.Id })|
    </p>
    }
@Html.ActionLink("Add Role", "RoleCreate")